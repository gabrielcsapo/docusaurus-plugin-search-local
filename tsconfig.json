{
  "compilerOptions": {
    "declaration": false,
    "declarationMap": false,
    "esModuleInterop": true,
    "importHelpers": true,
    "jsx": "preserve",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true,
    "typeRoots": ["node_modules/@types"],
    "types": ["node", "cheerio", "@docusaurus/module-type-aliases", "jest"]
  },
  "include": ["src"],
  "exclude": ["**/*.spec.ts", "**/*.test.ts", "src/client", "src/server"]
}
